@model KategoriEkleModel

<form asp-action="Ekle" asp-controller="Kategori" asp-area="Admin" method="post" class="w-75 mx-auto p-3" enctype="multipart/form-data">
    <div class="form-group">
        <label>Ürün Adı: </label>
        <input type="text" asp-for="Ad" class="form-control" />
        <span asp-validation-for="Ad" class="text-danger"></span>
    </div>

    <div class="form-group">
        <a asp-action="Index" asp-controller="Kategori" asp-area="Admin" class="btn btn-danger">Vazgeç</a>
        <button type="submit" class="btn btn-primary">Kaydet</button>
    </div>
</form>

@section JavaScript{
<script src="~/jqueryValid/jquery.validate.min.js.validate.min"></script>
<script src="~/jqueryUnob/jquery.validate.unobtrusive.min.js"></script>
}